{
  "name": "@etecture/rex",
  "version": "0.5.0",
  "description": "Exotic components, hooks and functions that you wouldn't find in the usual react component library.",
  "keywords": [
    "react",
    "components",
    "hooks",
    "utility"
  ],
  "homepage": "https://github.com/etecture/rex",
  "repository": {
    "type": "git",
    "url": "https://github.com/etecture/rex"
  },
  "author": "Rico Maier <rico.maier@etecture.de>",
  "license": "MIT",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run cleanup && rexb build src/index.ts",
    "cleanup": "rm -rf ./dist",
    "dev": "rexb build src/index.ts --watch"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.5.0",
    "clsx": "^2.1.1",
    "jotai": "^2.8.0"
  },
  "devDependencies": {
    "@etecture/rex-builder": "0.1.0",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.4.5"
  }
}
